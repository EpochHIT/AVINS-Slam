<launch>
    <!-- 启动相机、IMU重时间戳和DenseSurfelMapping -->
    <include file="$(find surfel_fusion)/launch/vins_realsense.launch"/>
    
    <!-- 启动VINS估计器 -->
    <node pkg="vins" type="vins_node" name="vins_estimator" output="screen" 
          args="$(find vins)/../config/realsense_d435i_fix/realsense_stereo_imu_config.yaml"/>
    
    <!-- 启动RViz可视化 -->
    <include file="$(find vins)/launch/vins_rviz.launch"/>
    
</launch>
